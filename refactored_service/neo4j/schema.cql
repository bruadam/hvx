// Basic Neo4j schema for HVX CODEX

CREATE CONSTRAINT IF NOT EXISTS
FOR (e:SpatialEntity) REQUIRE e.id IS UNIQUE;

CREATE CONSTRAINT IF NOT EXISTS
FOR (t:TimeSeries) REQUIRE t.id IS UNIQUE;

CREATE CONSTRAINT IF NOT EXISTS
FOR (s:Standard) REQUIRE s.id IS UNIQUE;

CREATE CONSTRAINT IF NOT EXISTS
FOR (a:Analysis) REQUIRE a.id IS UNIQUE;
